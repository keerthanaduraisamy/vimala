import java.util.*;
import java.io.*;
public class Even{
  
  public static void main(String args[])
  {
    
    int n;
    Scanner sc=new Scanner(System.in);
    n=sc.nextInt();
    if(n%2==0)
    System.out.println("the given input is even");
    else
    System.out.println("the given input is odd");
    }
    }
